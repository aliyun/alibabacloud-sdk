import Util;
import OpenApi;

import EndpointUtil;

extends OpenApi;

init(config: OpenApi.Config){
  super(config);
  @endpointRule = '';
  
  checkConfig(config);
  @endpoint = getEndpoint('umeng-finplus', @regionId, @endpointRule, @network, @suffix, @endpointMap, @endpoint);
}

function getEndpoint(productId: string, regionId: string, endpointRule: string, network: string, suffix: string, endpointMap: map[string]string, endpoint: string) throws: string{
  if (!Util.empty(endpoint)) {
    return endpoint;
  }

  if (!Util.isUnset(endpointMap) && !Util.empty(endpointMap[regionId])) {
    return endpointMap[regionId];
  }
  return EndpointUtil.getEndpointRules(productId, regionId, endpointRule, network, suffix);
}
model MoZiStatusCallBackRequest {
  body?: string(name='body', description='body'),
}

model MoZiStatusCallBackResponse = {
  headers: map[string]string(name='headers'),
  body: string(name='body'),
}

async function moZiStatusCallBack(request: MoZiStatusCallBackRequest): MoZiStatusCallBackResponse {
  var runtime = new Util.RuntimeOptions{};
  var headers : map[string]string = {};
  return moZiStatusCallBackWithOptions(request, headers, runtime);
}

async function moZiStatusCallBackWithOptions(request: MoZiStatusCallBackRequest, headers: map[string]string, runtime: Util.RuntimeOptions): MoZiStatusCallBackResponse {
  Util.validateModel(request);
  var req = new OpenApi.OpenApiRequest{ 
    headers = headers,
    body = request.body,
  };
  return doROARequest('MoZiStatusCallBack', '2021-11-25', 'HTTPS', 'POST', 'AK', `/mozi/status/callback`, 'string', req, runtime);
}

