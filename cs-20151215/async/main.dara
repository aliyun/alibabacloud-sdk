  minReplicaCount?: int32(name='min_replica_count', description='The minimum number of pods that must be guaranteed during scale-in activities.', example='0', position='Body'),
  recycleNodeDeletionEnabled?: boolean(name='recycle_node_deletion_enabled', description='Specifies whether to delete the corresponding Kubernetes node objects after nodes are removed in swift mode.', example='false', position='Body'),
